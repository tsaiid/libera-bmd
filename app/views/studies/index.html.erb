<h1>Studies#index</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Date</th>
      <th>Name</th>
      <th>ID</th>
      <th>Acc NO</th>
    </tr>
  </thead>
  <tbody>
    <% @studies.each do |study| %>
      <tr>
        <td><%= study.scan_date.to_date.to_s %></td>
        <td><%= study.patient.name %></td>
        <td><%= study.patient.pid %></td>
        <td><%= study.acc %></td>
      </tr>
    <% end %>
  </tbody>
</table>